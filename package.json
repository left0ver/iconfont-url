{
  "name": "iconfont-url",
  "type": "module",
  "version": "0.0.4",
  "description": "获取iconfont中项目图标的在线链接",
  "sideEffect": false,
  "author": "left0ver",
  "license": "MIT",
  "homepage": "https://github.com/left0ver/iconfont-url#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/left0ver/iconfont-url.git"
  },
  "bugs": "https://github.com/left0ver/iconfont-url/issues",
  "keywords": [
    "get-iconfont-url",
    "iconfont-url",
    "iconfont"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "esno src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "npx tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "pnpm build",
    "watch": "esno watch src/index.ts",
    "release": "release-it"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@left0ver/eslint-config-ts": "^0.0.2",
    "@types/lodash.pick": "^4.4.7",
    "@types/node": "^20.0.0",
    "eslint": "^8.31.0",
    "esno": "^0.16.3",
    "release-it": "^15.6.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4"
  }
}
